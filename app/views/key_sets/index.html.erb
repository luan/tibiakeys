<p id="notice"><%= notice %></p>

<h1>Key Sets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Vocation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @key_sets.each do |key_set| %>
      <tr>
        <td><%= key_set.name %></td>
        <td><%= key_set.vocation %></td>
        <td><%= link_to 'Show', key_set %></td>
        <td><%= link_to 'Edit', edit_key_set_path(key_set) %></td>
        <td><%= link_to 'Destroy', key_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Key Set', new_key_set_path %>
